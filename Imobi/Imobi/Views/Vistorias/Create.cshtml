@model Imobi.Models.ViewModels.Vistoria.Vistoria

@{
    ViewData["Title"] = "Create";
}

<h2>@ViewData["Title"]</h2>

<h2>Criar Vistoria</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Nome" class="control-label"></label>
                <input asp-for="Nome" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="DataVistoria" class="control-label"></label>
                <input asp-for="DataVistoria" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Observacao" class="control-label"></label>
                <input asp-for="Observacao" class="form-control" />
            </div>

            <div class="form-group">
                <label asp-for="Energia" class="control-label"></label>
                <input asp-for="Energia" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Agua" class="control-label"></label>
                <input asp-for="Agua" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Gas" class="control-label"></label>
                <input asp-for="Gas" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

